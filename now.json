{
  "name": "wg14-link",
  "version": 2,
  "routes": [
    { "src": "/404(-.+)?\\.html", "dest": "/404.html", "status": 404 },
    { "handle": "filesystem" },
    { "src": "/([a-zA-Z0-9]+)", "dest": "/api/redirect.js?id=$1" },
    { "src": "/(.*)", "dest": "/404.html", "status": 404 }
  ],
  "functions": {
    "api/redirect.js": {
      "includeFiles": "build/**"
    }
  }
}
